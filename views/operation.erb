
<!DOCTYPE html>
<html>
<head>

  <title>operation</title>
</head>
<body>

  <% if @operation == "subtract" %>
  <h1> subtaction <%=(@num1 - @num2).to_s %></h1>
  <% elsif @operation == "multiply" %>

  <h1> multiplies <%=(@num1 * @num2).to_s %></h1>
  
  <% elsif @operation == "divide" %>
  <h1> divides <%= (@num1/@num2).to_s %></h1>

  <% elsif @operation == "add" %>
  <h1> added <%= (@num1 +@num2).to_s %></h1>
  <% end  %>

</body>
</html>
